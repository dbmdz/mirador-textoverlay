{
  "name": "mirador-textoverlay",
  "version": "1.0.0-alpha.1",
  "description": "Mirador 4 plugin to render a hidden (but selectable) or visible text overlay",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npm run build:umd",
    "build:demo": "NODE_ENV=development webpack --mode=development",
    "build:umd": "NODE_ENV=production webpack --mode=production",
    "build:es": "mkdir -p es && cp -r src/* es && NODE_ENV=production MODULE_FORMAT=es npx babel es -d es",
    "build:cjs": "mkdir -p lib && cp -r src/* lib && NODE_ENV=production MODULE_FORMAT=cjs npx babel lib -d lib",
    "lint": "eslint ./src ./__tests__",
    "format": "eslint --fix ./src ./__tests__",
    "prepublishOnly": "npm run build",
    "parcel": "parcel demo/src/index.html",
    "test": "npm run lint && jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "start": "NODE_ENV=development webpack serve --open"
  },
  "peerDependencies": {
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.x",
    "@mui/icons-material": "^5.x",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "manifesto.js": "^4.2.0",
    "mirador": "^4.0.0-alpha.1",
    "react": "18.x",
    "react-dom": "18.x",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-regenerator": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "@jest/globals": "^29.7.0",
    "@mui/icons-material": "^5.16.6",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "canvas": "^2.11.2",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.4.3",
    "mirador": "^4.0.0-alpha.1",
    "prettier": "^3.3.3",
